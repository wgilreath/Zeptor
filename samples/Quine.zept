package xyz.zepton.samples;

prog Quine {

  begin {

    char q = 34;
    String[] l = {
      "prog Quine",
      "{",
      "  begin",
      "  {",
      "    char q = 34;",
      "    String[] l = {",
      "    ",
      "    };",
      "    for(int i = 0; i < 6; i++)           ",
      "        println(l[i]);",
      "    for(int i = 0; i < l.length; i++)    ",
      "        println(l[6] + q + l[i] + q + ',');",
      "    for(int i = 7; i < l.length; i++)    ",
      "        println(l[i]);",
      "  }",
      "}",
      ""
    };

    for (int i = 0; i < 6; i++) {
      println(l[i]);
    }//end for

    for (int i = 0; i < l.length; i++) {
      println(l[6] + q + l[i] + q + ',');
    }//end for

    for (int i = 7; i < l.length; i++) {
      println(l[i]);
    }//end for

  }//end begin

}//end prog Quine
